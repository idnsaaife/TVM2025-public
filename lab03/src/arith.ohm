Arithmetic {
  Exp
    = AddExp

  AddExp
    = MulExp (("+" | "-") MulExp)*

  MulExp
    =  PriExp (("*" | "/") PriExp)*


  PriExp
    = "-" PriExp        -- neg     
    | "(" Exp  ")"      -- paren
    | number 			-- num
    | ident				-- id      

  number
    = digit+                     

  ident
    = letter alnum*         
}